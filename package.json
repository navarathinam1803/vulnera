{
  "name": "risklens",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "An MCP developer security assistant that scans dependencies, answers questions, prioritizes fixes, and translates technical security output into actionable guidance",
  "scripts": {
    "dev": "nitrostack-cli dev",
    "build": "nitrostack-cli build",
    "start": "npm run build && nitrostack-cli start",
    "start:prod": "nitrostack-cli start",
    "upgrade": "nitrostack-cli upgrade",
    "install:all": "nitrostack-cli install",
    "widget": "npm --prefix src/widgets"
  },
  "keywords": [
    "nitrostack",
    "mcp",
    "pizza",
    "map",
    "widgets"
  ],
  "author": "Nava & Shiva",
  "license": "MIT",
  "dependencies": {
    "@nitrostack/core": "^1",
    "dotenv": "^16.3.1",
    "jspdf": "^4.1.0",
    "jspdf-autotable": "^5.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nitrostack/cli": "^1",
    "@types/node": "^22.10.0",
    "typescript": "^5.3.3"
  }
}